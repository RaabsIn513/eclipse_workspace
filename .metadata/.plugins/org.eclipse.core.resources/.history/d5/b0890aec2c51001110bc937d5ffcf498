package school.work;

public class Select2 {

	public static void main(String[] args)
	{
		System.out.println("hell0");
	}
	
	public int fastmedian(int[] a)
	{
		int t = 0;
		int y = 0;
		// swap a[2] with a[1];
		if (a[1]<a[2])
		    t=a[2]; a[2]=a[1]; a[1]=t;
		// swap a[4] with a[3];
		if (a[3]<a[4])
		    t=a[3]; a[3]=a[4]; a[4]=t;

		// swap a[1] with a[3] and swap a[2] with a[4]
		if (a[1] < a[3])
		{
			t=a[1]; a[1]=a[3]; a[3]=t;
		    t=a[2]; a[2]=a[4]; a[4]=t;
		}

		// swap a[2] with a[5]
		if (a[2] < a[5])
		      t=a[2]; a[2]=a[5]; a[5]=t;

		if (a[2]>a[3])
		{	    
			if (a[3]>a[5])
		        y=a[3];
		    else
		        y=a[5];
		}
		else if (a[2]>a[4])
		{        
			y=a[2];
		}
		else
			y=a[4];
		return y;
	}
}
